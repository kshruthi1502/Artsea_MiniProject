package artsea_proj;
import javax.swing.*;
import java.awt.*;
public class Artist_gid {

	JFrame frame;
	JLabel id;
	JTextField tid;
	JButton viewopt;
	Artist_gid(JFrame frame)
	{
		this.frame=frame;
		id = new JLabel("Id");
        id.setFont(new Font("Arial", Font.PLAIN, 15));
        id.setSize(100, 20);
        id.setLocation(100, 100);
        tid = new JTextField();
        tid.setFont(new Font("Arial", Font.PLAIN, 15));
        tid.setSize(190, 20);
        tid.setLocation(200, 100);
        frame.add(tid);
        viewopt=new JButton("DELETE");
        viewopt.setBounds(230,210,100,50);
        viewopt.setBorder(BorderFactory.createLineBorder(Color.BLACK));
	}
}
